{
  "packages": {
    "packages/config": {
      "component": "eslint-config"
    },
    "packages/plugin": {
      "component": "eslint-plugin"
    }
  },
  "plugins": [
    {
      "type": "node-workspace",
      "merge": true
    },
    {
      "type": "linked-versions",
      "group-name": "eslint",
      "groupName": "eslint",
      "components": ["eslint-config", "eslint-plugin"]
    }
  ]
}
